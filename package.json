{
  "name": "src",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "build": "webpack --config webpack/webpack.config.js",
    "build-fast": "npm-run-all --parallel build-server build-client",
    "build-server": "webpack --config webpack/server.config.js",
    "build-client": "webpack --config webpack/client.config.js",
    "build-production": "PORT=8585 NODE_ENV=production npm-run-all build-fast",
    "watch": "npm-run-all --parallel watch:client watch:server",
    "watch:client": "webpack --watch --config webpack/webpack.config.js",
    "watch:server": "nodemon \"./dist/server.js\" --watch \"./dist\"",
    "run-server": "nodemon \"./dist/server.js\"",
    "start": "npm-run-all watch:cleint watch:server",
    "server-forever": "forever start dist/server.js",
    "kill-forever": "forever list && forever stopall",
    "production": "npm-run-all build-production run-server",
    "dev": "PORT=3000 npm-run-all build-fast watch"
  },
  "author": "Ildar <ildarrazin@gmail.com>",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "axios": "^0.15.3",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.22.0",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.27.3",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "forever": "^0.15.3",
    "html-webpack-plugin": "^2.28.0",
    "html-webpack-template": "^6.0.1",
    "logger": "0.0.1",
    "nodemon": "^1.11.0",
    "normalize.css": "^5.0.0",
    "npm-run-all": "^4.0.2",
    "ramda": "^0.24.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-html5video": "^2.2.0",
    "react-inlinesvg": "^0.5.5",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "redux": "^3.6.0",
    "redux-act": "^1.1.1",
    "redux-logger": "^2.8.2",
    "redux-promise-middleware": "^4.2.0",
    "redux-thunk": "^2.2.0",
    "semantic-ui-css": "^2.2.10",
    "semantic-ui-react": "^0.70.0",
    "style-loader": "^0.14.0",
